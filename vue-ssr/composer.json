{
    "name": "radiantabyss/lumi-vue-ssr-boilerplate",
    "type": "project",
    "description": "Lumi Vue SSR Boilerplate",
    "keywords": [],
    "license": "proprietary",
    "authors": [
        {
            "name": "Robert Buchi",
            "email": "robert@planetweb.ro"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "jaybizzle/crawler-detect": "^1.2",
        "laravel/framework": "10.*",
        "radiantabyss/lumi-vue-artisan": "1.*",
        "radiantabyss/lumi-laravel-core": "1.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "ssr/app/"
        },
        "files": [
            "ssr/app/Support/Helpers/BaseHelper.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
